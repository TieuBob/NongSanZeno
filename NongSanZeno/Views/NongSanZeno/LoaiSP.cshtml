@model IEnumerable<NongSanZeno.Models.tbLoaiSanPham>

<h2 style="text-align:center">LOẠI NÔNG SẢN</h2>
<ul>
    @foreach (var item in Model)
    {
        <li>
            <a href="@Url.Action("SPTheoLoai", "NongSanZeno", new {id = item.MaLoaiSP})" class="list-group-item">
                @item.TenLoaiSP
            </a>
        </li>
    }
</ul>